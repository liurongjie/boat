.container{
  position: absolute;
  width: 100%;
  height: 100%;
  background-color: white;
}
.boat{
  position: relative;
  width: 300rpx;
  height: 70rpx;
  border-radius: 40rpx;
  background:linear-gradient(to bottom,#FDB38F,#F7907E);
  margin-left: 225rpx;
  margin-top: -240rpx;
  text-align:center;
  line-height: 70rpx;
  color:rgba(255,255,255,0.8);
}
.cicle{
  position: relative;
  width: 190rpx;
  height: 190rpx;
  border-radius: 100rpx;
