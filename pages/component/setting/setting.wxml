<!--pages/component/setting/setting.wxml-->
<view class='container'>
  <image src='../../../static/pic/new.png' style='position:reative;width:100%;height:40%;'></image>
  
  <image src='../../../static/icon/setting/right.svg' class='rightcicle'></image>
  <image src='../../../static/icon/setting/left.svg' class='leftcicle'></image>
  <view class='point1'></view>
  <view class='point2'></view>
  <view class='point3'></view>
  <notverify wx:if="{{list==0}}"></notverify>
  <list wx:if="{{list==1}}" bindtap='changelist' ></list>
  <list1 wx:if="{{list==2}}"></list1>
  <list2 wx:if="{{list==3}}"></list2>
  <list3 wx:if="{{list==4}}"></list3>
  <list4 wx:if="{{list==5}}"></list4>
  <list5 wx:if="{{list==6}}"></list5>
</view>
