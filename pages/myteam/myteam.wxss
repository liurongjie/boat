.background {
  position: fixed;
  width: 100%;
  height: 90%;
}

.helplist {
  position: relative;
  width: 100%;
  height: fit-content;
}

.title {
  display: flex;
  position: relative;
  width: 100%;
  height: 120rpx;
  font-size: 35rpx;
  color: rgba(132, 136, 132, 0.781);
  font-weight: 600;
   justify-content: center;
  align-items: center;
}
.team{
  position: absolute;
  top: 20vh;
  width: 60%;
  left: 20%;
  height: 60%;
  display: flex;
  flex-direction: column;
  
}
.showmodel{
  width: 80%;
  height: 52%;
  background: white;
  position: absolute;
  left: 10%;
  top: 25vh;
   border-radius: 0 0 20rpx 20rpx;
}
.cap{
  position: absolute;
  width: 86%;
 display: flex;
 justify-content: center;
  height: 20%;
  background: linear-gradient(to bottom right,#5EE4FE,#57ABFE);
  margin-top:30%;
  bottom:8%;
  border-radius: 0 0 20rpx 20rpx;
}
.bac{
  position: absolute;
  width: 100rpx;
  height: 100rpx;
  
  right:-50rpx;
   top: -50rpx;
   z-index: 10;
}