.background {
  position: fixed;
  width: 100%;
  height: 1100rpx;
}

.helplist {
  position: relative;
  width: 100%;
  height: fit-content;
}

.title {
  display: flex;
  position: relative;
  width: 100%;
  height: 120rpx;
  font-size: 35rpx;
  color: rgba(132, 136, 132, 0.781);
  font-weight: 600;
   justify-content: center;
  align-items: center;
}
.team{
  position: absolute;
  top: 15vh;
  width: 60%;
  left: 20%;
  height: 70%;
  display: flex;
  flex-direction: column;
}
.showmodel{
  width: 80%;
  height: 60%;
  background: white;
  position: absolute;
  left: 10%;
  top: 21vh;
   border-radius: 0 0 20rpx 20rpx;
}
.cap{
  position: absolute;
  width: 90%;
 display: flex;
 justify-content: center;
  height: 20%;
  background-color: blue;
  margin-top:30%;
  top:30%;
  border-radius: 0 0 20rpx 20rpx;
}
.bac{
  position: absolute;
  width: 100rpx;
  height: 100rpx;
  background: black;
  right:-50rpx;
   top: -50rpx;
   z-index: 10;
}